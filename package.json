{
  "name": "@mentorpass/factoria",
  "version": "0.0.0-development",
  "description": "Simplistic model factory for Node/JavaScript",
  "license": "MIT",
  "repository": "mentorpass/factoria",
  "author": {
    "name": "Phan An",
    "email": "me@phanan.net",
    "url": "https://phanan.net"
  },
  "scripts": {
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "jest",
    "build": "rollup -c",
    "commit": "cz"
  },
  "main": "dist/factoria.js",
  "types": "./types.d.ts",
  "keywords": [
    "model",
    "factory",
    "seed",
    "dummy",
    "schema"
  ],
  "peerDepedencies": {
    "@faker-js/faker": "^6.0.0-alpha.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@faker-js/faker": "^6.0.0-alpha.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-typescript": "^4.1.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "cz-conventional-changelog": "^3.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "rollup": "^2.38.5",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.2",
    "ts-jest": "^26.5.1",
    "tslib": "^1.11.1",
    "typescript": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts": "yarn lint"
  },
  "dependencies": {
    "is-plain-object": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "repositoryUrl": "https://github.com/LawsonTaylor/factoria",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
